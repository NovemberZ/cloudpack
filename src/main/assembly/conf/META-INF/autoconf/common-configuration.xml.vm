<?xml version="1.0"?>
<configuration>
	<!-- protocol -->
	<property>
		<name>com.alibaba.hbase.replication.protocol.version</name>
		<value>${replication_protocol_version}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.protocol.class</name>
		<value>${replication_protocol_class}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.protocol.adapter.hdfs.fs</name>
		<value>${replication_protocol_adapter_hdfs_fs}</value>
	</property>

	<property>
		<name>com.alibaba.hbase.replication.protocol.adapter.hdfs.dir.root</name>
		<value>${replication_protocol_adapter_hdfs_dir_root}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.protocol.adapter.hdfs.dir.targetpath</name>
		<value>${replication_protocol_adapter_hdfs_dir_targetpath}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.protocol.adapter.hdfs.dir.oldpath</name>
		<value>${replication_protocol_adapter_hdfs_dir_oldpath}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.protocol.adapter.hdfs.dir.rejectpath</name>
		<value>${replication_protocol_adapter_hdfs_dir_rejectpath}</value>
	</property>
	<!-- protocol end -->
	
	<property>
		<name>com.alibaba.hbase.replication.zoo.lock.root</name>
		<value>${replication_zookeeper_lock_root}</value>
	</property>
	
	<property>
		<name>dfs.permissions</name>
		<value>false</value>
	</property>
</configuration>